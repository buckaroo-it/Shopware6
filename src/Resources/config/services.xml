            <argument type="service" id="Shopware\Core\Checkout\Payment\PaymentProcessor"/>
            <argument type="service" id="order_address.repository"/>
        </service>

        <service id="Buckaroo\Shopware6\Subscribers\CheckoutConfirmTemplateSubscriber" >
